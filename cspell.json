{
  "version": "0.2",
  "language": "en",
  "words": [
    "commitlint",
    "lintstagedrc",
    "localci",
    "Rieck",
    "tsparser"
  ],
  "flagWords": [],
  "ignorePaths": [
    "node_modules",
    "docs",
    "dist",
    "out",
    "build",
    "coverage",
    "*.log",
    "pnpm-lock.yaml",
    "package.json",
    "package-lock.json",
    ".git",
    "*.tsbuildinfo"
  ],
  "ignoreRegExpList": [
    "/\\b[a-f0-9]{32,}\\b/gi",
    "/\\b[A-Z]{2,}_[A-Z_]+\\b/g"
  ]
}
